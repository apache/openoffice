<meta>
<topic id="textsharedexplorerdatabasepasswordxhp" indexer="include" status="PUBLISH">
<title id="tit" xml-lang="en-US">User Name and Password Required</title>
<filename>/text/shared/explorer/database/password.xhp</filename>
</topic>
<history>
