<meta>
      <topic id="textsbasicshared03132100xml" indexer="include" status="PUBLISH">
         <title xml-lang="en-US" id="tit">GetGuiType Function [Runtime]</title>
         <filename>/text/sbasic/shared/03132100.xhp</filename>
