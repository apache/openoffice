      <topic id="textshared0105220000xml" indexer="include" status="PUBLISH">
         <title xml-lang="en-US" id="tit">Text</title>
         <filename>/text/shared/01/05220000.xhp</filename>
      </topic>
   </meta>
   <body>
      <section id="text">
<bookmark xml-lang="en-US" branch="index" id="bm_id3146856"><bookmark_value>text; text/draw objects</bookmark_value>
         <bookmark_value>draw objects; text in</bookmark_value>
         <bookmark_value>frames; text fitting to frames</bookmark_value>
</bookmark>
<bookmark xml-lang="en-US" branch="hid/.uno:TextAttributes" id="bm_id463689" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/.uno:TextAttributes" id="bm_id3160463" localize="false"/>
<bookmark xml-lang="en-US" branch="hid/CUI_HID_PAGE_TEXTATTR" id="bm_id3153514" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3146856" role="heading" level="1" l10n="U" oldref="1"><link href="text/shared/01/05220000.xhp" name="Text">Text</link></paragraph>
         <paragraph xml-lang="en-US" id="par_id3150279" role="paragraph" l10n="U" oldref="2"><ahelp hid="HID_PAGE_TEXTATTR">Sets the layout and anchoring properties for text in the selected drawing or text object.</ahelp></paragraph>
      </section>
      <section id="howtoget">
         <embed href="text/shared/00/00040502.xhp#text"/>
      </section>
      <paragraph xml-lang="en-US" id="par_id3154794" role="paragraph" l10n="U" oldref="4">The text is positioned relative to the edges of the drawing or text object.</paragraph>
      <paragraph xml-lang="en-US" id="hd_id3149031" role="heading" level="2" l10n="U" oldref="5">Text</paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_AUTOGROW_WIDTH" id="bm_id3154230" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3150445" role="heading" level="3" l10n="U" oldref="7">Fit width to text</paragraph>
      <paragraph xml-lang="en-US" id="par_id3145629" role="paragraph" l10n="CHG" oldref="8"><ahelp hid="SVX_TRISTATEBOX_RID_SVXPAGE_TEXTATTR_TSB_AUTOGROW_WIDTH">Expands the width of the object to the width of the text, if the object is smaller than the text.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_AUTOGROW_HEIGHT" id="bm_id3146109" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3149511" role="heading" level="3" l10n="U" oldref="9">Fit height to text</paragraph>
      <paragraph xml-lang="en-US" id="par_id3149640" role="paragraph" l10n="CHG" oldref="10"><ahelp hid="SVX_TRISTATEBOX_RID_SVXPAGE_TEXTATTR_TSB_AUTOGROW_HEIGHT">Expands the height of the object to the height of the text, if the object is smaller than the text.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_FIT_TO_SIZE" id="bm_id3150144" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3152867" role="heading" level="3" l10n="U"
                 oldref="11">Fit to frame</paragraph>
      <paragraph xml-lang="en-US" id="par_id3147834" role="paragraph" l10n="U" oldref="12"><ahelp hid="SVX_TRISTATEBOX_RID_SVXPAGE_TEXTATTR_TSB_FIT_TO_SIZE">Resizes the text to fit the entire area of the drawing or text object.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_CONTOUR" id="bm_id3155942" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3155892" role="heading" level="3" l10n="CHG"
                 oldref="29">Adjust to contour</paragraph>
      <paragraph xml-lang="en-US" id="par_id3153577" role="paragraph" l10n="U" oldref="30"><ahelp hid="SVX_TRISTATEBOX_RID_SVXPAGE_TEXTATTR_TSB_CONTOUR">Adapts the text flow so that it matches the contours of the selected drawing object.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_WORDWRAP_TEXT" id="bm_id8065736" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN10705" role="heading" level="3" l10n="NEW">Word wrap text in shape</paragraph>
      <paragraph xml-lang="en-US" id="par_idN10709" role="paragraph" l10n="NEW"><ahelp hid=".">Wraps the text that you add after double-clicking a custom shape to fit inside the shape.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_AUTOGROW_SIZE" id="bm_id8341929" localize="false"/>
<paragraph xml-lang="en-US" id="par_idN10720" role="heading" level="3" l10n="NEW">Resize shape to fit text</paragraph>
      <paragraph xml-lang="en-US" id="par_idN10724" role="paragraph" l10n="NEW"><ahelp hid=".">Resizes a custom shape to fit the text that you enter after double-clicking the shape.</ahelp></paragraph>
      <paragraph xml-lang="en-US" id="hd_id3154288" role="heading" level="2" l10n="CHG"
                 oldref="13">Spacing to borders</paragraph>
      <paragraph xml-lang="en-US" id="par_id3151265" role="paragraph" l10n="U" oldref="14">Specify the amount of space to leave between the edges of the drawing or text object and the borders of the text.</paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:MetricField:RID_SVXPAGE_TEXTATTR:MTR_FLD_LEFT" id="bm_id3149812" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3150443" role="heading" level="3" l10n="U"
                 oldref="15">Left</paragraph>
      <paragraph xml-lang="en-US" id="par_id3156113" role="paragraph" l10n="U" oldref="16"><ahelp hid="SVX_METRICFIELD_RID_SVXPAGE_TEXTATTR_MTR_FLD_LEFT">Enter the amount of space to leave between the left edge of the drawing or text object and the left border of the text.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:MetricField:RID_SVXPAGE_TEXTATTR:MTR_FLD_RIGHT" id="bm_id3143271" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3155419" role="heading" level="3" l10n="U"
                 oldref="17">Right</paragraph>
      <paragraph xml-lang="en-US" id="par_id3155388" role="paragraph" l10n="U" oldref="18"><ahelp hid="SVX_METRICFIELD_RID_SVXPAGE_TEXTATTR_MTR_FLD_RIGHT">Enter the amount of space to leave between the right edge of the drawing or text object and the right border of the text.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:MetricField:RID_SVXPAGE_TEXTATTR:MTR_FLD_TOP" id="bm_id3145227" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3148926" role="heading" level="3" l10n="U"
                 oldref="19">Top</paragraph>
      <paragraph xml-lang="en-US" id="par_id3157808" role="paragraph" l10n="U" oldref="20"><ahelp hid="SVX_METRICFIELD_RID_SVXPAGE_TEXTATTR_MTR_FLD_TOP">Enter the amount of space to leave between the top edge of the drawing or text object and the upper border of the text.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:MetricField:RID_SVXPAGE_TEXTATTR:MTR_FLD_BOTTOM" id="bm_id3150130" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3149237" role="heading" level="3" l10n="U"
                 oldref="21">Bottom</paragraph>
      <paragraph xml-lang="en-US" id="par_id3159342" role="paragraph" l10n="U" oldref="22"><ahelp hid="SVX_METRICFIELD_RID_SVXPAGE_TEXTATTR_MTR_FLD_BOTTOM">Enter the amount of space to leave between the bottom edge of the drawing or text object and the lower border of the text.</ahelp></paragraph>
      <paragraph xml-lang="en-US" id="hd_id3149192" role="heading" level="2" l10n="U"
                 oldref="23">Text anchor</paragraph>
      <paragraph xml-lang="en-US" id="par_id3155179" role="paragraph" l10n="U" oldref="24">Set the anchor type and the anchor position.</paragraph>
<bookmark xml-lang="en-US" branch="hid/CUI_HID_TEXTATTR_CTL_POSITION" id="bm_id3149955" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3154381" role="heading" level="3" l10n="U"
                 oldref="25">Graphic field</paragraph>
      <paragraph xml-lang="en-US" id="par_id3155504" role="paragraph" l10n="U" oldref="26"><ahelp hid="HID_TEXTATTR_CTL_POSITION">Click where you want to place the anchor for the text.</ahelp></paragraph>
<bookmark xml-lang="en-US" branch="hid/cui:TriStateBox:RID_SVXPAGE_TEXTATTR:TSB_FULL_WIDTH" id="bm_id3149763" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3155323" role="heading" level="3" l10n="U"
                 oldref="27">Full width</paragraph>
      <paragraph xml-lang="en-US" id="par_id3150244" role="paragraph" l10n="CHG" oldref="28"><ahelp hid=".">Anchors the text to the full width of the drawing object or text object.</ahelp></paragraph>
   </body>
