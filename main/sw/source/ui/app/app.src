	{
        Pos = MAP_APPFONT ( 12, 14 ) ;
        Size = MAP_APPFONT ( 103, 32 ) ;
    };
    FixedText ST_MERGE_SOURCE_UNAVAILABLE
    {
        Pos = MAP_APPFONT ( 12, 6 ) ;
        Size = MAP_APPFONT ( 178, 40 ) ;
        WordBreak = TRUE ;
