#~ msgctxt "03091507.xhp#par_id3162086.490.help.text"
#~ msgid "Symbol in Formula Elements Window"
#~ msgstr "Symbol i fönstret Formelelement"

#~ msgctxt "03091501.xhp#par_id3151388.472.help.text"
