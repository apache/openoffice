msgid "Legend"
msgstr "Förklaring"

#: 04020000.xhp#bm_id3156441.help.text
msgid ""
"<bookmark_value>chart legends; hiding</bookmark_value><bookmark_value>hiding;"
"chart legends</bookmark_value>"
