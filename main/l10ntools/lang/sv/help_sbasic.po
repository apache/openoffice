msgctxt "03060200.xhp#par_id3147434.14.help.text"
msgid "A = 10: B = 8: C = 6: D = Null"
msgstr "A = 10: B = 8: C = 6: D = Null"

#: 03060200.xhp#par_id3152462.15.help.text
