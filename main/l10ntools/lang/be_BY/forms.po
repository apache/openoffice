#: xforms.src#RID_STR_DATATYPE_TIME.string.text
msgid "Time"
msgstr "Час"

#: xforms.src#RID_STR_DATATYPE_URL.string.text
msgid "Hyperlink"
msgstr "Сеціўная спасылка"

#: xforms.src#RID_STR_DATATYPE_YEAR.string.text
msgid "Year"
