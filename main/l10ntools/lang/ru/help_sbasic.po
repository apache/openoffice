#: 03010100.xhp#tit.help.text
msgid "Display Functions"
msgstr "Функции отображения"

#: 03010100.xhp#hd_id3151384.1.help.text
msgid ""
"<link href=\"text/sbasic/shared/03010100.xhp\" name=\"Display "
"Functions\">Display Functions</link>"
msgstr ""
"<link href=\"text/sbasic/shared/03010100.xhp\" name=\"Функции "
