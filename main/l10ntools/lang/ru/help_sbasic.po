#: 20000000.xhp#par_id1261940.help.text
msgid ""
"<ahelp hid=\".uno:ManageLanguage\">Opens a <link "
"href=\"text/sbasic/guide/translation.xhp\">dialog</link> to enable or manage "
"multiple sets of dialog resources for multiple languages.</ahelp>"
msgstr ""
