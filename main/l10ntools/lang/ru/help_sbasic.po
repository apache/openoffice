msgid "iColor = 7"
msgstr "iColor = 7"

#: 03010304.xhp#par_id3145230.32.help.text
msgid ""
"sText = \"RGB= \" & Red(QBColor( iColor) ) & \":\" & Blue(QBColor( iColor) ) & "
"\":\" & Green(QBColor( iColor) )"
msgstr ""
"sText = \"RGB= \" & Red(QBColor( iColor) ) & \":\" & Blue(QBColor( iColor) ) & "
"\":\" & Green(QBColor( iColor) )"

