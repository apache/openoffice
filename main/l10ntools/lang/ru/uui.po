msgstr "Домен: ${DOMAIN}, Путь: ${PATH}, Cookie: ${COOKIE}.\n"

#: cookiedg.src#STR_COOKIES_SEND_TITLE.string.text
msgid "Send Cookies"
msgstr "Отправить файлы Cookie"

