msgstr "End Sub"

#: 03060600.xhp#tit.help.text
msgid "Xor-Operator [Runtime]"
msgstr "Xor-operaattori [ajonaikainen]"

#: 03060600.xhp#bm_id3156024.help.text
msgid "<bookmark_value>Xor operator (logical)</bookmark_value>"
msgstr ""
"<bookmark_value>Xor-operaattori "
"(looginen)</bookmark_value><bookmark_value>Xor operator "
"(logical)</bookmark_value>"

#: 03060600.xhp#hd_id3156024.1.help.text
msgid ""
"<link href=\"text/sbasic/shared/03060600.xhp\" name=\"Xor-Operator [Runtime"
"]\">Xor-Operator [Runtime]</link>"
msgstr ""
"<link href=\"text/sbasic/shared/03060600.xhp\" name=\"Xor-Operator [Runtime"
"]\">Xor-operaattori [ajonaikainen]</link>"

#: 03060600.xhp#par_id3159414.2.help.text
msgid "Performs a logical Exclusive-Or combination of two expressions."
msgstr ""
"XOR suorittaa eksklusiivisen OR-operaation (poissulkeva tai) kahden "
"lausekkeen yhdistelmälle."

#: 03060600.xhp#hd_id3153381.3.help.text
msgctxt "03060600.xhp#hd_id3153381.3.help.text"
msgid "Syntax:"
msgstr "Syntaksi:"

