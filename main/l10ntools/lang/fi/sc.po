msgid "new text"
msgstr "uusi teksti"

#: scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_REPLACE.9.string.text
msgid "The text to be inserted."
msgstr "Lisättävä teksti."

#: scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_FIXED.1.string.text
