"<bookmark_value>deleting;visible cells only</bookmark_value>      "
"<bookmark_value>invisible cells</bookmark_value>      "
"<bookmark_value>filters;copying visible cells only</bookmark_value>      "
"<bookmark_value>hidden cells</bookmark_value>"
msgstr ""
"<bookmark_value>células; copiar/excluir/formatar/mover</bookmark_value><book"
"mark_value>linhas;visíveis e "
"invisíveis</bookmark_value><bookmark_value>copiar; somente células "
"visíveis</bookmark_value><bookmark_value>formatar;somente células "
"visíveis</bookmark_value><bookmark_value>mover;somente células "
