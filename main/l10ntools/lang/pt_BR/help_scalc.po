msgstr "ASEN"

#: 04060106.xhp#par_id3156296.91.help.text
msgid ""
"<ahelp hid=\"HID_FUNC_ARCSIN\">Returns the inverse trigonometric sine of a "
"number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_FUNC_ARCSIN\">Retorna o seno trigonométrico inverso (arco "
"seno) de um número.</ahelp>"

#: 04060106.xhp#hd_id3149716.92.help.text
