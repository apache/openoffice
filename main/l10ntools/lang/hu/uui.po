msgctxt "cookiedg.src#STR_COOKIES_SEND_COOKIES.string.text"
msgid "Domain: ${DOMAIN}, Path: ${PATH}, Cookie: ${COOKIE}.\n"
msgstr "Tartomány: ${DOMAIN}, Elérési út: ${PATH}, Süti: ${COOKIE}.\n"

