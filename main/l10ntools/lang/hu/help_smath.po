msgstr "<link href=\"text/smath/01/05010000.xhp\" name=\"Betűk\">Betűk</link>"

#: main0105.xhp#hd_id3154656.4.help.text
msgid ""
"<link href=\"text/smath/01/05020000.xhp\" name=\"Font Size\">Font Size</link>"
msgstr ""
"<link href=\"text/smath/01/05020000.xhp\" name=\"Betűméret\">Betűméret</link>"

#: main0105.xhp#hd_id3154484.5.help.text
msgid "<link href=\"text/smath/01/05030000.xhp\" name=\"Spacing\">Spacing</link>"
msgstr "<link href=\"text/smath/01/05030000.xhp\" name=\"Térköz\">Térköz</link>"

