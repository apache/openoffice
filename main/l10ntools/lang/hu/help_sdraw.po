#~ "name=\"Szemcseppentő\">Szemcseppentő</link>"

#: 04010000.xhp#tit.help.text
msgid "Insert Page"
msgstr "Oldal beszúrása"

#: 04010000.xhp#hd_id3150202.1.help.text
msgid ""
"<link href=\"text/sdraw/01/04010000.xhp\" name=\"Insert Page\">Insert "
"Page</link>"
msgstr ""
"<link href=\"text/sdraw/01/04010000.xhp\" name=\"Oldal beszúrása\">Oldal "
"beszúrása</link>"

#: 04010000.xhp#par_id3152988.2.help.text
msgid ""
"<variable id=\"seitetext\">Inserts a blank page after the selected "
"page.</variable>"
msgstr ""
