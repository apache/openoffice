msgid "Sub ExampleFileLen"
msgstr "Sub ExampleFileLen"

#: 03020408.xhp#par_id3145271.12.help.text
msgid "msgbox FileLen(\"C:\\autoexec.bat\")"
msgstr "msgbox FileLen(\"C:\\autoexec.bat\")"

