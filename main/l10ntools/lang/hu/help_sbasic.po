msgid "Syntax:"
msgstr "Szintaxis:"

#: 03020408.xhp#par_id3149656.4.help.text
msgid "FileLen (Text As String)"
msgstr "FileLen (Szöveg As String)"

#: 03020408.xhp#hd_id3148798.5.help.text
