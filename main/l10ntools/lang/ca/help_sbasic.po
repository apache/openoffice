msgstr "c2 = a1 & \": \" & a2 & chr(13)"

#: 03030206.xhp#par_id3150749.41.help.text
msgid "c2 = c2 & b1 & \": \" & b2 & chr(13)"
msgstr "c2 = c2 & b1 & \": \" & b2 & chr(13)"

#: 03030206.xhp#par_id3154755.42.help.text
msgid ""
