msgctxt "03132400.xhp#par_idN105A2.help.text"
msgid "Syntax:"
msgstr "Sintaxi:"

#: 03132400.xhp#par_idN105A6.help.text
msgid "oObj = CreateObject( type )"
msgstr "oObj = CreateObject( type )"

