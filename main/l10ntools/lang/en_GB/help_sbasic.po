msgstr ""

#: source/text/sbasic/shared/01170101.xhp#hd_id3153965.82
msgid  "Date format"
msgstr "Date format"

#: source/text/sbasic/shared/01170101.xhp#par_id3155334.83
msgid  "\<ahelp hid=\".\"\>Specify the desired format for a date control. A date control interprets the user input depending on this format setting.\</ahelp\>"
msgstr ""

#: source/text/sbasic/shared/01170101.xhp#hd_id3154663.121
msgid  "Date max."
msgstr "Date latest"

#: source/text/sbasic/shared/01170101.xhp#par_id3148485.122
msgid  "\<ahelp hid=\".\"\>Specify the upper limit for a date control.\</ahelp\>"
msgstr ""

#: source/text/sbasic/shared/01170101.xhp#hd_id3152778.131
