msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3143271.2
msgid  "Declares a variable or an array."
msgstr "Declares a variable or an array."

#: source/text/sbasic/shared/03102100.xhp#par_id3154686.3
msgid  "If the variables are separated by commas (for example, DIM sPar1, sPar2, sPar3 AS STRING), only Variant variables can be defined. Use a separate definition line for each variable."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3156422.4
msgid  "DIM sPar1 AS STRING"
msgstr "DIM sPar1 AS STRING"

#: source/text/sbasic/shared/03102100.xhp#par_id3159252.5
msgid  "DIM sPar2 AS STRING"
msgstr "DIM sPar2 AS STRING"

#: source/text/sbasic/shared/03102100.xhp#par_id3153142.6
msgid  "DIM sPar3 AS STRING"
msgstr "DIM sPar3 AS STRING"

#: source/text/sbasic/shared/03102100.xhp#par_id3152576.7
msgid  "Dim declares local variables within subroutines. Global variables are declared with the PUBLIC or the PRIVATE statement."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#hd_id3156443.8
msgid  "Syntax:"
msgstr "Syntax:"

#: source/text/sbasic/shared/03102100.xhp#par_id3149412.9
msgid  "[ReDim]Dim VarName [(start To end)] [As VarType][, VarName2 [(start To end)] [As VarType][,...]]"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#hd_id3147397.10
msgid  "Parameters:"
msgstr "Parameters:"

#: source/text/sbasic/shared/03102100.xhp#par_id3154730.11
msgid  "\<emph\>VarName:\</emph\> Any variable or array name."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3147125.12
msgid  "\<emph\>Start, End:\</emph\> Numerical values or constants that define the number of elements (NumberElements=(end-start)+1) and the index range."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3153877.13
msgid  "Start and End can be numerical expressions if ReDim is applied at the procedure level."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3153510.14
msgid  "\<emph\>VarType:\</emph\> Key word that declares the data type of a variable."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3154015.15
msgid  "\<emph\>Keyword:\</emph\> Variable type"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3153949.16
msgid  "\<emph\>Bool:\</emph\> Boolean variable (True, False)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3156275.17
msgid  "\<emph\>Currency:\</emph\> Currency-Variable (Currency with 4 Decimal places)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3156057.18
msgid  "\<emph\>Date:\</emph\> Date variable"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3148405.19
msgid  "\<emph\>Double:\</emph\> Double-precision floating-point variable (1,79769313486232x10E308 - 4,94065645841247 x 10E-324)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3148916.20
msgid  "\<emph\>Integer:\</emph\> Integer variable (-32768 - 32767)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3150045.21
msgid  "\<emph\>Long:\</emph\> Long integer variable (-2.147.483.648 - 2.147.483.647)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3149255.22
msgid  "\<emph\>Object:\</emph\> Object variable (Note: this variable can only subsequently be defined with Set!)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3155937.23
msgid  "\<emph\>Single:\</emph\> Single-precision floating-point variable (3,402823 x 10E38 - 1,401298 x 10E-45)."
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3151251.24
msgid  "\<emph\>String:\</emph\> String variable consisting of a maximum of 64,000 ASCII characters."
