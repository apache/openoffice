msgstr "ElseIf sDate > sToday Then"

#: source/text/sbasic/shared/03090101.xhp#par_id3146912.23
msgid  "MsgBox "The expiration date has not yet passed""
msgstr ""

#: source/text/sbasic/shared/03090101.xhp#par_id3153710.24
msgid  "Else"
msgstr "Else"

#: source/text/sbasic/shared/03090101.xhp#par_id3154754.25
msgid  "MsgBox "The expiration date is today""
msgstr ""

#: source/text/sbasic/shared/03090101.xhp#par_id3154361.26
