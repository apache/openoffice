msgctxt "03020408.xhp#hd_id3150768.7.help.text"
msgid "Parameters:"
msgstr "Parametre:"

#: 03020408.xhp#par_id3153193.8.help.text
msgctxt "03020408.xhp#par_id3153193.8.help.text"
