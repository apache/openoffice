msgid "Object Catalog"
msgstr "Каталог на обектите"

#: 11090000.xhp#hd_id3153255.1.help.text
msgid ""
"<link href=\"text/sbasic/shared/02/11090000.xhp\" name=\"Object Catalog\">Object "
"Catalog</link>"
msgstr ""
