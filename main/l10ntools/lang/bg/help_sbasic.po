msgid "Next iCount"
msgstr "Next iCount"

#: 03102101.xhp#par_id3149926.39.help.text
msgctxt "03102101.xhp#par_id3149926.39.help.text"
msgid "end sub"
msgstr "End Sub"

#: 03102200.xhp#tit.help.text
