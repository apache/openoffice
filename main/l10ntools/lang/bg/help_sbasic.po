#: 20000000.xhp#par_id3147244.43.help.text
msgid "<ahelp hid=\".uno:InsertNumericField\">Adds a numeric field.</ahelp>"
msgstr "<ahelp hid=\".uno:InsertNumericField\">Добавя числово поле.</ahelp>"

#: 20000000.xhp#hd_id3149870.44.help.text
msgid "Currency Field"
msgstr "Поле за валута"

#: 20000000.xhp#par_id3153958.help.text
msgid ""
"<image id=\"img_id3150435\" src=\"res/commandimagelist/sc_currencyfield.png\" "
"width=\"0.1665inch\" height=\"0.1665inch\"><alt "
