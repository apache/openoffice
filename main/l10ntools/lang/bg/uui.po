msgid "Domain: ${DOMAIN}, Path: ${PATH}, Cookie: ${COOKIE}.\n"
msgstr "Домен: ${DOMAIN}, Път: ${PATH}, Бисквитки: ${COOKIE}.\n"

#: cookiedg.src#STR_COOKIES_SEND_TITLE.string.text
msgid "Send Cookies"
msgstr "Изпращане на бисквитки"

