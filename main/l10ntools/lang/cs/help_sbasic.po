msgstr "Dim a1, b1, c1, a2, b2, c2 as String"

#: 03030206.xhp#par_id3149378.33.help.text
msgid "a1 = \"start time\""
msgstr "a1 = \"start time\""

#: 03030206.xhp#par_id3145800.34.help.text
msgid "b1 = \"end time\""
msgstr "b1 = \"end time\""

#: 03030206.xhp#par_id3151074.35.help.text
