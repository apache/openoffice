msgstr "Syntaxe:"

#: 03020103.xhp#par_id3154124.4.help.text
msgid ""
"Open FileName As String [For Mode] [Access IOMode] [Protected] As "
"[#]FileNumber As Integer [Len = DatasetLength]"
msgstr ""
"Open FileName As String [For Mode] [Access IOMode] [Protected] As "
"[#]FileNumber As Integer [Len = DatasetLength]"

#: 03020103.xhp#hd_id3156280.5.help.text
