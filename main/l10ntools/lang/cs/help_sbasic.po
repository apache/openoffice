msgstr "Povolit kukátko"

#: 11080000.xhp#hd_id3154863.1.help.text
msgid ""
"<link href=\"text/sbasic/shared/02/11080000.xhp\" name=\"Enable Watch\">Enable "
"Watch</link>"
msgstr ""
