#~ msgstr "Symbol w oknie Elementy formuły"

#~ msgctxt "03091507.xhp#par_id3162086.490.help.text"
#~ msgid "Symbol in Formula Elements Window"
#~ msgstr "Symbol w oknie Elementy formuły"

#~ msgctxt "03091501.xhp#par_id3151388.472.help.text"
#~ msgid "Symbol in Formula Elements Window"
#~ msgstr "Symbol w oknie Elementy formuły"

#~ msgctxt "03091506.xhp#par_id3167610.487.help.text"
#~ msgid "Symbol in Formula Elements Window"
#~ msgstr "Symbol w oknie Elementy formuły"

#~ msgctxt "03091508.xhp#par_id3180684.493.help.text"
#~ msgid "Symbol in Formula Elements Window"
#~ msgstr "Symbol w oknie Elementy formuły"

#~ msgid ""
#~ "<ahelp hid=\"SID_ZOOMOUT\">Decreases the display scale of formulas by 25%"
#~ ".</ahelp> The zoom scale is displayed on the status bar. You can also change "
#~ "the scale in the <link href=\"text/shared/00/00000001.xhp#kontextmenue\" "
#~ "name=\"context menu\">context menu</link> of the status bar. The workspace "
#~ "context menu also has zoom options."
