msgid "Sub1:"
msgstr ""

#: 03090303.xhp#par_id3150750.22.help.text
msgid "Sub2:"
msgstr ""

#: 03090303.xhp#par_id3155067.24.help.text
msgid "Line1:"
msgstr ""

#: 03090303.xhp#par_id3150321.25.help.text
msgid "sVar =sVar & \" Label 1\" : GoTo Ende"
msgstr ""

#: 03090303.xhp#par_id3149019.26.help.text
msgid "Line2:"
msgstr ""

