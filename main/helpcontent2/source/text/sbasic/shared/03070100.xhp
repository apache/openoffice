<helpdocument version="1.0">
<meta>
<topic id="textsbasicshared03070100xml" indexer="include" status="PUBLISH">
<title id="tit" xml-lang="en-US">"-" Operator [Runtime]</title>
<filename>/text/sbasic/shared/03070100.xhp</filename>
</topic>
