<bookmark xml-lang="en-US" branch="hid/SD_HID_SD_NAVIGATOR_TBI_DRAGTYPE" id="bm_id3150258" localize="false"/><paragraph role="heading" id="hd_id3149979" xml-lang="en-US" level="2" l10n="U" oldref="23">Drag Mode</paragraph>
