<helpdocument version="1.0">
<meta>
<topic id="textshared0105040300xml" indexer="include" status="PUBLISH">
<title xml-lang="en-US" id="tit">Header</title>
<filename>/text/shared/01/05040300.xhp</filename>
</topic>
