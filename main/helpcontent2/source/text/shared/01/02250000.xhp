<bookmark xml-lang="en-US" branch="hid/.uno:Bib/DeleteRecord" id="bm_id3157846" localize="false"/>
<paragraph xml-lang="en-US" id="par_id3153947" role="paragraph" l10n="U" oldref="48"><ahelp hid="." visibility="hidden">Deletes the current record.</ahelp></paragraph>
