<bookmark xml-lang="en-US" branch="hid/svx:MetricField:RID_SVXPAGE_GRID:MTR_FLD_DRAW_Y" id="bm_id3156280" localize="false"/>
<paragraph xml-lang="en-US" id="hd_id3150447" role="heading" level="3" l10n="U"
oldref="17">Vertical</paragraph>
<paragraph xml-lang="en-US" id="par_id3148923" role="paragraph" l10n="U" oldref="18"><ahelp hid="SVX:METRICFIELD:RID_SVXPAGE_GRID:MTR_FLD_DRAW_Y">Defines the grid points spacing in the desired unit of measurement on the Y-axis.</ahelp></paragraph>
<paragraph xml-lang="en-US" id="hd_id3147228" role="heading" level="2" l10n="U"
oldref="37">Subdivision</paragraph>
